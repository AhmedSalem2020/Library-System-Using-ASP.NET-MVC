@model IEnumerable<ELibrary.Models.book>

@{
    ViewBag.Title = "Booksincategory";
    Layout = "~/Views/Shared/_Layout - Copy.cshtml";
}
<br />

<br />
<div class="form-group about-info">
    Number Of Books : @ViewBag.book_count
</div>

<table class="table table-hover table-responsive">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.joinDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.joinDate)
                </td>
            </tr>

        }
    </tbody>
</table>


